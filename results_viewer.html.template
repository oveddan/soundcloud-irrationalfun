<html>
  <head>
    <style type='text/css'>
      table {
        border-spacing: 0;
        border-collapse: collapse;
        margin-bottom: 30px;
      }
      table td {
        width: 5px;
        height: 5px;
        padding: 0;
      }
      .a0 {
        background-color: #ffffff;
      }
      .a1 {
        background-color: #f0f0f0;
      }
      .a2 {
        background-color: #ebebeb;
      }
      .a3 {
        background-color: #d0d0d0;
      }
      .a4 {
        background-color: #d0d0d0;
      }
      .a5 {
        background-color: #a8a8a8;
      }
      .a6 {
        background-color: #878787;
      }
      .a7 {
        background-color: #535353;
      }
      .a8 {
        background-color: #333333;
      }
      .a9 {
        background-color: #000000;
      }
    </style>
  </head>
  <body>
    <script id="grid_template" type="text/x-handlebars-template">
      <table>
        {{#rows}}
          <tr>
            {{#row}}
              <td class='a{{.}}'>
              </td>
            {{/row}}
          </tr>
        {{/rows}}
      </table>
    </script>


    <h1>Calculation results:</h1>
    <h3>Time elapsed: {timeElapsed}</h3>
    <h3>Closest sequences:</h3>
    <div id='results'>
    </div>

    <script type="text/javascript" src="http://code.jquery.com/jquery-2.1.1.min.js" ></script>
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/handlebars.js/2.0.0-alpha.2/handlebars.min.js"></script>

    <script type='text/javascript' >

      $(document).ready(function(){

        var results = getResults();

        var source   = $("#grid_template").html();
        var template = Handlebars.compile(source);

        for(var i = 0; i < results.length; i++){
          var result = results[i];

          var formattedResult = { rows : [] };
          for(var j = 0; j < result.length; j++){
            formattedResult.rows.push({row: result[j]});
          }

          var html = template(formattedResult);

          $('#results').append(html);
        }
      });

      var getResults = function(){
        return {results};
      }
    </script>
  </body>
</html>